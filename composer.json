{
  "name": "miaoxing/m",
  "type": "miaoxing-plugin",
  "description": "Miaoxing mobile plugin",
  "authors": [
    {
      "name": "twinh",
      "email": "twinhuang@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Miaoxing\\M\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\M\\": "tests"
    }
  },
  "require": {
    "miaoxing/plugin": "^0.14.4"
  },
  "require-dev": {
    "miaoxing/dev": "^9.1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 2.x-dev"
    }
  }
}
